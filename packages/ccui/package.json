{
  "name": "vue3-ccui01",
  "version": "1.0.0",
  "license": "MIT",
  "description": "vue3-ccui01 components based on Vite and Vue3",
  "keywords": [
    "frontend",
    "typescript",
    "ui-component",
    "components",
    "vue-components",
    "vue",
    "vue3",
    "vite",
    "jsx",
    "vue3-ccui01"
  ],
  "homepage": "https://vaebe.github.io/ccui/",
  "repository": {
    "type": "git",
    "url": "https://github.com/vaebe/ccui.git"
  },
  "main": "vue-ccui.umd.js",
  "module": "vue-ccui.es.js",
  "style": "style.css",
  "type": "module",
  "scripts": {
    "dev": "vitepress dev docs",
    "dev:site": "vite --port 3011",
    "predev": "node ../cli/index.js create -t ccui --ignore-parse-error && node ../cli/index.js generate:theme",
    "build:components:dts": "vue-tsc --declaration --emitDeclarationOnly",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "register:components": "vitepress-rc",
    "build": "pnpm generate:theme && node --max-old-space-size=8192 node_modules/vitepress/bin/vitepress.js build docs && cp public/* docs/.vitepress/dist/assets && cp docs/assets/* docs/.vitepress/dist/assets",
    "build:lib": "pnpm predev -- -e prod && pnpm build:components && pnpm release",
    "test": "jest --config jest.config.js",
    "coverage": "jest --config jest.config.js --coverage",
    "build:components": "node ../cli/index.js build",
    "generate:theme": "node ../cli/index.js generate:theme",
    "generate:dts": "node ../cli/index.js generate:dts",
    "release": "node ../cli/index.js release",
    "cli": "node ../cli/index.js create",
    "cli:create": "node ../cli/index.js create -t component",
    "prebuild": "node ../cli/index.js create -t ccui --ignore-parse-error",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "dependencies": {
    "@devui-design/icons": "^1.4.0",
    "ccui-theme": "workspace:^",
    "clipboard-copy": "^4.0.1",
    "vitepress-theme-demoblock": "^2.0.2",
    "vue": "^3.4.31"
  },
  "devDependencies": {
    "@docsearch/css": "^3.6.1",
    "@docsearch/js": "^3.6.1",
    "@types/markdown-it": "^14.1.1",
    "@types/node": "^20.1.4",
    "@types/vue": "^2.0.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/test-utils": "^2.3.2",
    "c8": "^7.13.0",
    "chalk": "^4.1.2",
    "escape-html": "^1.0.3",
    "fflate": "^0.8.2",
    "jsdom": "^22.0.0",
    "lodash": "^4.17.21",
    "markdown-it": "^14.1.0",
    "markdown-it-container": "^4.0.0",
    "moment": "^2.29.4",
    "path-exists": "^5.0.0",
    "prismjs": "^1.29.0",
    "sass": "^1.62.1",
    "typescript": "^5.0.4",
    "vite": "^4.3.5",
    "vite-svg-loader": "^3.6.0",
    "vitepress": "^1.3.1",
    "vitest": "^0.31.0",
    "vue-tsc": "^1.6.5"
  }
}
